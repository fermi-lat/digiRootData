#====================================================
${CONSTITUENT}:: ${CINTFILE}
	@echo "------> ${CONSTITUENT} ok"

VPATH=../digiRootData/
${CINTFILE}:: ${root_headers}
	@echo " running rootcint to generate ${CINTFILE} "
	cd ../${package}/ ; export LD_LIBRARY_PATH=${ROOT_PATH}/lib; ${ROOT_PATH}/bin/rootcint -f ${CINTFILE} -c -I${MCROOTDATAROOT} ${root_headers}
#==================================================== 
