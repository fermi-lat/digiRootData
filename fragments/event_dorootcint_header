#====================================================
${CONSTITUENT}:: ${CINTFILE}
	@echo "------> ${CONSTITUENT} ok"

VPATH=../digiRootData/
${CINTFILE}:: ${root_headers}
	@echo " running rootcint to generate ${CINTFILE} "
	cd ../${package}/ ; ${ROOT_PATH}/bin/rootcint -f ${CINTFILE} -c ${root_headers}
#==================================================== 
